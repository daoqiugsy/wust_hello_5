<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wust_hello.mapper.LeisureMapper">
    <select id="selectLeisure" resultType="int">
        select count(stu_id) from leisure where time_slot=#{time_slot} and date=#{date};
    </select>
</mapper>
